{
  "mcpServers": {
    "doi-mcp": {
      "command": "direnv",
      "args": [
        "exec",
        ".",
        "doi-mcp"
      ]
    },
    "jupyter": {
      "command": "direnv",
      "args": [
        "exec",
        ".",
        "jupyter-mcp-server"
      ],
      "env": {
        "ALLOW_IMG_OUTPUT": "true"
      }
    }
  }
}