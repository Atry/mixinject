- [Builtin, NatEquality]
- [Builtin, NatPlus]
- [Builtin, NatToPython]
- [Builtin, BooleanToPython]

  # =============================================================================
  # Concrete Church numerals
  # =============================================================================

- Zero:
    - [ArithmeticTest, ~, NatFactory, Zero]

  One:
    - [ArithmeticTest, ~, NatFactory, Successor]
    - predecessor:
        - [Zero]

  Two:
    - [ArithmeticTest, ~, NatFactory, Successor]
    - predecessor:
        - [One]

  Three:
    - [ArithmeticTest, ~, NatFactory, Successor]
    - predecessor:
        - [Two]

  Four:
    - [ArithmeticTest, ~, NatFactory, Successor]
    - predecessor:
        - [Three]

  Five:
    - [ArithmeticTest, ~, NatFactory, Successor]
    - predecessor:
        - [Four]

  Six:
    - [ArithmeticTest, ~, NatFactory, Successor]
    - predecessor:
        - [Five]

  Seven:
    - [ArithmeticTest, ~, NatFactory, Successor]
    - predecessor:
        - [Six]

  # =============================================================================
  # Addition tests
  # =============================================================================

  # 3 + 4
  threePlusFour:
    - [Three, Plus]
    - addend:
        - [Four]

  # 5 + 2
  fivePlusTwo:
    - [Five, Plus]
    - addend:
        - [Two]

  # 0 + 3
  zeroPlusThree:
    - [Zero, Plus]
    - addend:
        - [Three]

  # =============================================================================
  # Direct equality tests (without addition)
  # =============================================================================

  # 0 == 0
  zeroEqualsZero:
    - [Zero, Equal]
    - other:
        - [Zero]

  # 3 == 3
  threeEqualsThree:
    - [Three, Equal]
    - other:
        - [Three]

  # 3 == 4
  threeEqualsFour:
    - [Three, Equal]
    - other:
        - [Four]

  # =============================================================================
  # Equality tests with addition results
  # =============================================================================

  # (3 + 4) == 7
  threePlusFourEqualsSeven:
    - [Seven, Equal]
    - other: [threePlusFour, sum]

  # (5 + 2) == 7
  fivePlusTwoEqualsSeven:
    - [Seven, Equal]
    - other: [fivePlusTwo, sum]

  # (5 + 2) == 2
  fivePlusTwoEqualsTwo:
    - [Two, Equal]
    - other: [fivePlusTwo, sum]

  # (0 + 3) == 3
  zeroPlusThreeEqualsThree:
    - [Three, Equal]
    - other: [zeroPlusThree, sum]
